{
	"name": "dataflow1",
	"properties": {
		"description": "dataflow one description",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "CosmosDbNoSql1",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SearchCriteria as string,",
				"          Results as (Make_ID as short, Make_Name as string)[],",
				"          Count as short,",
				"          Message as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     container: 'cars',",
				"     storeType: 'oltp',",
				"     format: 'document',",
				"     store: 'cosmosDB') ~> source1",
				"source1 select(mapColumn(",
				"          SearchCriteria,",
				"          Results,",
				"          Count,",
				"          Message",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}